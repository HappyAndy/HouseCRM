<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="chart" namespace="/chart" extends="myDefault">
        <action name="customer">
            <result>/WEB-INF/pages/chart/customer.jsp</result>
        </action>
        <action name="sexCount" class="chartController" method="sexCount"/>
        <action name="isUsed" class="chartController" method="isUsed"/>
        <action name="type" class="chartController" method="type"/>
        <action name="condition" class="chartController" method="condition"/>
        <action name="source" class="chartController" method="source"/>
        <action name="user">
            <result>/WEB-INF/pages/chart/user.jsp</result>
        </action>
        <action name="users" class="chartController" method="user"/>
        <action name="diploma" class="chartController" method="diploma"/>
    </package>
</struts>